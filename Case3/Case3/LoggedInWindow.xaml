<Window x:Class="Case3.LoggedInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Case3"
        mc:Ignorable="d"
        Title="LoggedInWindow" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center">
        <Label Content="Click the button to add data to Uniconta" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="PopulateBtn" Content="Populate Uniconta"  VerticalAlignment="Center" HorizontalAlignment="Center" Padding="10 5 10 5" Click="PopulateBtn_Click"/>
    </StackPanel>
</Window>
